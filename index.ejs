<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="<%=htmlWebpackPlugin.files.chunks.main.css %>">
    <% htmlWebpackPlugin.options.themeChunks.forEach(function(themeChunk){%>
        <link rel="alternate stylesheet" href="<%=htmlWebpackPlugin.files.chunks[themeChunk].css %>" type="text/css" title="<%=themeChunk %>" theme="<%=themeChunk %>">
    <% }) %>
</head>

<body>
    <div id="app"></div>
    <script type="text/javascript" src="<%=htmlWebpackPlugin.files.chunks.main.entry %>"></script>
    <% htmlWebpackPlugin.options.themeChunks.forEach(function(themeChunk){%>
        <script type="text/javascript" src="<%=htmlWebpackPlugin.files.chunks[themeChunk].entry %>"></script>
    <% }) %>
</body>

</html>
